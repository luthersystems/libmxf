{
  "transform": {
    "^.+\\.(t|j)sx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "setupFilesAfterEnv": ["<rootDir>/src/__tests__/jest.setup.ts"],
  "testPathIgnorePatterns": ["\\.d.ts$", "/jest.setup.ts"]
}
